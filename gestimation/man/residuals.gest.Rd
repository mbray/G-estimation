## The function is currently defined as
function (obj, j = NULL) 
{
    if (is.null(obj$residuals)) {
        warning("Residuals were not stored.  Use keep=TRUE to store residuals")
        return(NULL)
    }
    if (is.null(j)) {
        obj$residuals
    }
    else {
        obj$residuals[[j]]
    }
  }
}
